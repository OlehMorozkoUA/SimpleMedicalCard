<Window x:Class="RegistrationForm.MedicalCard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegistrationForm"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        Title="Medical Card" Height="800" Width="1000" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" WindowStyle="None" Icon="Images/Medical.ico">
    <Window.Resources>
        <Style x:Key="TabMedicalCard">
            <Setter Property="Control.Background">
                <Setter.Value>
                    <LinearGradientBrush>
                        <GradientStop Color="#FF646464" Offset="0.05"/>
                        <GradientStop Color="#FF747070" Offset="0.1"/>
                        <GradientStop Color="#FF646464" Offset="0.15"/>
                        <GradientStop Color="#FF646464" Offset="0.2"/>
                        <GradientStop Color="#FF747070" Offset="0.25"/>
                        <GradientStop Color="#FF646464" Offset="0.3"/>
                        <GradientStop Color="#FF646464" Offset="0.35"/>
                        <GradientStop Color="#FF747070" Offset="0.4"/>
                        <GradientStop Color="#FF646464" Offset="0.45"/>
                        <GradientStop Color="#FF646464" Offset="0.5"/>
                        <GradientStop Color="#FF747070" Offset="0.55"/>
                        <GradientStop Color="#FF646464" Offset="0.6"/>
                        <GradientStop Color="#FF646464" Offset="0.65"/>
                        <GradientStop Color="#FF747070" Offset="0.7"/>
                        <GradientStop Color="#FF646464" Offset="0.75"/>
                        <GradientStop Color="#FF646464" Offset="0.8"/>
                        <GradientStop Color="#FF747070" Offset="0.85"/>
                        <GradientStop Color="#FF646464" Offset="0.9"/>
                    </LinearGradientBrush>
                </Setter.Value>
            </Setter>
            <Setter Property="Control.Effect">
                <Setter.Value>
                    <DropShadowEffect
                        ShadowDepth="14"
                        Direction="41"
                        Color="Black"
                        Opacity="0.7"/>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="{x:Type TextBox}">
            <Style.Setters>
                <Setter Property="BorderThickness" Value="0 0 0 1"/>
                <Setter Property="BorderBrush" Value="Black"/>
                <Setter Property="Foreground" Value="Black"/>
                <Setter Property="Background" Value="Transparent"/>
                <Setter Property="CaretBrush" Value="Orange"/>
            </Style.Setters>

            <Style.Triggers>
                <Trigger Property="IsEnabled" Value="false">
                    <Setter Property="Foreground">
                        <Setter.Value>
                            <LinearGradientBrush>
                                <GradientStop Color="#FFFFC500" Offset="0.3"/>
                                <GradientStop Color="#FFFFE800" Offset="0.7"/>
                                <GradientStop Color="#FFFFC500" Offset="0.9"/>
                            </LinearGradientBrush>
                        </Setter.Value>
                    </Setter>
                    <Setter Property="FontWeight" Value="Bold"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.Background>
            <LinearGradientBrush>
                <GradientStop Color="#FF646464" Offset="0.05"/>
                <GradientStop Color="#FF747070" Offset="0.1"/>
                <GradientStop Color="#FF646464" Offset="0.15"/>
                <GradientStop Color="#FF646464" Offset="0.2"/>
                <GradientStop Color="#FF747070" Offset="0.25"/>
                <GradientStop Color="#FF646464" Offset="0.3"/>
                <GradientStop Color="#FF646464" Offset="0.35"/>
                <GradientStop Color="#FF747070" Offset="0.4"/>
                <GradientStop Color="#FF646464" Offset="0.45"/>
                <GradientStop Color="#FF646464" Offset="0.5"/>
                <GradientStop Color="#FF747070" Offset="0.55"/>
                <GradientStop Color="#FF646464" Offset="0.6"/>
                <GradientStop Color="#FF646464" Offset="0.65"/>
                <GradientStop Color="#FF747070" Offset="0.7"/>
                <GradientStop Color="#FF646464" Offset="0.75"/>
                <GradientStop Color="#FF646464" Offset="0.8"/>
                <GradientStop Color="#FF747070" Offset="0.85"/>
                <GradientStop Color="#FF646464" Offset="0.9"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Border BorderBrush="Black" BorderThickness="2" VerticalAlignment="Top" MouseMove="Border_MouseMove">
            <Border.Effect>
                <DropShadowEffect/>
            </Border.Effect>
            <Border.Background>
                <LinearGradientBrush>
                    <GradientStop Color="#FF646464" Offset="0.3"/>
                    <GradientStop Color="#FFC1B6B6" Offset="0.5"/>
                    <GradientStop Color="#FF646464" Offset="0.7"/>
                </LinearGradientBrush>
            </Border.Background>
            <Grid VerticalAlignment="Top">
                <Button x:Name="ButtonClose" Click="ButtonClose_Click" Width="20" Height="20" HorizontalAlignment="Right" Margin="15 5" BorderBrush="Black"
                        Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}"
                        ToolTip="MaterialDesignFloatingActionMiniLightButton">
                    <Button.Effect>
                        <DropShadowEffect/>
                    </Button.Effect>
                    <Button.Background>
                        <LinearGradientBrush>
                            <GradientStop Color="#FF918D8D" Offset="0.3"/>
                            <GradientStop Color="#FFCBC7C7" Offset="0.5"/>
                            <GradientStop Color="#FF918D8D" Offset="0.7"/>
                        </LinearGradientBrush>
                    </Button.Background>
                    <materialDesign:PackIcon Kind="Close"/>
                </Button>
                <Button x:Name="ButtonHidden" Click="ButtonHidden_Click" Width="20" Height="20" HorizontalAlignment="Right" Margin="55 5" BorderBrush="Black"
                        Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}"
                        ToolTip="MaterialDesignFloatingActionMiniLightButton">
                    <Button.Effect>
                        <DropShadowEffect/>
                    </Button.Effect>
                    <Button.Background>
                        <LinearGradientBrush>
                            <GradientStop Color="#FF918D8D" Offset="0.3"/>
                            <GradientStop Color="#FFCBC7C7" Offset="0.5"/>
                            <GradientStop Color="#FF918D8D" Offset="0.7"/>
                        </LinearGradientBrush>
                    </Button.Background>
                    <materialDesign:PackIcon Kind="Minus"/>
                </Button>
                <Image Source="Images/Medical.ico" Height="30" Width="30" HorizontalAlignment="Left" Margin="15 5">
                    <Image.Effect>
                        <DropShadowEffect/>
                    </Image.Effect>
                </Image>
                <TextBlock Text="MedicalCard" 
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center" 
                           FontSize="25" FontWeight="Bold" 
                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                           x:Name="HeaderText"/>
            </Grid>
        </Border>
        <Grid Margin="0 70 0 0">
            <TabControl Margin="20" Style="{StaticResource TabMedicalCard}">
                <TabControl.ItemContainerStyle>
                    <Style TargetType="{x:Type TabItem}">
                        <Style.Setters>
                            <Setter Property="BorderThickness" Value="0"/>
                            <Setter Property="Padding" Value="0" />
                            <Setter Property="HeaderTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <Border>
                                            <Border.Background>
                                                <LinearGradientBrush>
                                                    <GradientStop Color="#FF646464" Offset="0.05"/>
                                                    <GradientStop Color="#FF747070" Offset="0.1"/>
                                                    <GradientStop Color="#FF646464" Offset="0.15"/>
                                                    <GradientStop Color="#FF646464" Offset="0.2"/>
                                                    <GradientStop Color="#FF747070" Offset="0.25"/>
                                                    <GradientStop Color="#FF646464" Offset="0.3"/>
                                                    <GradientStop Color="#FF646464" Offset="0.35"/>
                                                    <GradientStop Color="#FF747070" Offset="0.4"/>
                                                    <GradientStop Color="#FF646464" Offset="0.45"/>
                                                    <GradientStop Color="#FF646464" Offset="0.5"/>
                                                    <GradientStop Color="#FF747070" Offset="0.55"/>
                                                    <GradientStop Color="#FF646464" Offset="0.6"/>
                                                    <GradientStop Color="#FF646464" Offset="0.65"/>
                                                    <GradientStop Color="#FF747070" Offset="0.7"/>
                                                    <GradientStop Color="#FF646464" Offset="0.75"/>
                                                    <GradientStop Color="#FF646464" Offset="0.8"/>
                                                    <GradientStop Color="#FF747070" Offset="0.85"/>
                                                    <GradientStop Color="#FF646464" Offset="0.9"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                            <ContentPresenter>
                                                <ContentPresenter.Content>
                                                    <TextBlock x:Name="TextHeader" Margin="5" FontSize="20"
                                                               FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                               Text="{TemplateBinding Content}"/>
                                                </ContentPresenter.Content>
                                            </ContentPresenter>
                                        </Border>
                                        <DataTemplate.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type TabItem}},Path=IsSelected}" Value="True">
                                                <Setter TargetName="TextHeader" Property="Foreground">
                                                    <Setter.Value>
                                                        <LinearGradientBrush>
                                                            <GradientStop Color="#FFFF8000" Offset="0.3"/>
                                                            <GradientStop Color="Orange" Offset="0.5"/>
                                                            <GradientStop Color="#FFFF8000" Offset="0.7"/>
                                                        </LinearGradientBrush>
                                                    </Setter.Value>
                                                </Setter>
                                                <Setter TargetName="TextHeader" Property="FontWeight" Value="bold"/>
                                                <Setter TargetName="TextHeader" Property="Effect">
                                                    <Setter.Value>
                                                        <DropShadowEffect ShadowDepth="8" Opacity="0.8" Direction="41"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </DataTemplate.Triggers>
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style.Setters>
                    </Style>
                </TabControl.ItemContainerStyle>
                <TabItem Header="Cardiovascular">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="0.5*"/>
                            <ColumnDefinition Width="0.5*"/>
                            <ColumnDefinition Width="0.5*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="7*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Grid Grid.Column="0" Grid.ColumnSpan="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="0*"/>
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="2*"/>
                                <RowDefinition Height="3*"/>
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="0" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <TextBlock Grid.Row="0" Text="1.What pressure you have?"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Left"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Foreground="Yellow"/>

                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <RadioButton GroupName="QuestionOfCardiovascular1"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="less than 120/80"
                                                 Margin="10 0 10 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular1_1"
                                                 Click="QuestionOfCardiovascular1_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular1"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="140-159/90-99"
                                                 Margin="10 0 10 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular1_2"
                                                 Click="QuestionOfCardiovascular1_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular1"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="160-179/100-109"
                                                 Margin="10 0 10 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular1_3"
                                                 Click="QuestionOfCardiovascular1_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular1"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="grater than 180/110"
                                                 Margin="10 0 10 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular1_4"
                                                 Click="QuestionOfCardiovascular1_Click"/>
                                </StackPanel>
                            </Grid>

                            <Grid Grid.Row="1" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <TextBlock Grid.Row="0" Text="2.Are you experiencing visual impairment?"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Left"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial" Margin="0,-1,0,25" Grid.RowSpan="2"
                                           Foreground="Yellow"/>

                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="85,0">
                                    <RadioButton GroupName="QuestionOfCardiovascular2"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="Yes"
                                                 Margin="50 0 50 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular2_1"
                                                 Click="QuestionOfCardiovascular2_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular2"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="No"
                                                 Margin="50 0 50 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular2_2"
                                                 Click="QuestionOfCardiovascular2_Click"/>
                                </StackPanel>
                            </Grid>

                            <Grid Grid.Row="2" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <TextBlock Grid.Row="0" Text="3.What is the nature of the pain"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Left"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Foreground="Yellow"/>

                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <RadioButton GroupName="QuestionOfCardiovascular3"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="Nothing"
                                                 Margin="20 0 20 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular3_1"
                                                 Click="QuestionOfCardiovascular3_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular3"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="5-10min at physical activity"
                                                 Margin="20 0 20 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular3_2"
                                                 Click="QuestionOfCardiovascular3_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular3"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="5-10min at rest"
                                                 Margin="20 0 20 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular3_3"
                                                 Click="QuestionOfCardiovascular3_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular3"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="the symptoms increase"
                                                 Margin="20 0 20 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular3_4"
                                                 Click="QuestionOfCardiovascular3_Click"/>
                                </StackPanel>
                            </Grid>

                            <Grid Grid.Row="3" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <TextBlock Grid.Row="0" Text="4. What is your heart rate?"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Left"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Foreground="Yellow"/>

                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <RadioButton GroupName="QuestionOfCardiovascular4"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="40-60"
                                                 Margin="50 0 50 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular4_1"
                                                 Click="QuestionOfCardiovascular4_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular4"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="60-90"
                                                 Margin="50 0 50 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular4_2"
                                                 Click="QuestionOfCardiovascular4_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular4"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="90-160"
                                                 Margin="50 0 50 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular4_3"
                                                 Click="QuestionOfCardiovascular4_Click"/>
                                    <RadioButton GroupName="QuestionOfCardiovascular4"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 FontSize="20"
                                                 FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                 Content="160-220"
                                                 Margin="50 0 50 0"
                                                 Foreground="WhiteSmoke"
                                                 x:Name="QuestionOfCardiovascular4_4"
                                                 Click="QuestionOfCardiovascular4_Click"/>
                                </StackPanel>
                            </Grid>

                            <StackPanel Grid.ColumnSpan="4" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="5">
                                <TextBlock Text="Сonclusion: "
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Left"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Foreground="Yellow"/>
                                <TextBox x:Name="TextBoxConclusionCardiovascular" Width="700" Height="30" 
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Center"
                                           Grid.Column="0" Grid.Row="1"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Margin="50 20 40 20"
                                           TextAlignment="Center"/>   
                            </StackPanel>

                            <StackPanel Grid.ColumnSpan="4" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="6">
                                <TextBlock Text="Recipe: "
                                           VerticalAlignment="Top"
                                           HorizontalAlignment="Left"
                                           FontSize="20"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Foreground="Yellow"/>
                                <RichTextBox x:Name="TextBoxReceptionCardiovascular"
                                           Height="70"
                                           Width="850"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Center"
                                           Grid.Column="0" Grid.Row="1"
                                           FontSize="15"
                                           FontFamily="Technical Italic, Comic Sans MS, Arial"
                                           Margin="10"
                                           Block.LineHeight="2"
                                           VerticalScrollBarVisibility="Auto"/>
                            </StackPanel>

                            <Border Grid.Row="7" Grid.ColumnSpan="4" BorderBrush="Black" BorderThickness="2">
                                <ListView x:Name="List1" ScrollViewer.HorizontalScrollBarVisibility="Hidden" SelectionChanged="List1_SelectionChanged">
                                    <ListView.View>
                                        <GridView>
                                            <GridViewColumn Width="50">
                                                <TextBlock Foreground="Orange" FontSize="15" FontWeight="Bold">Id</TextBlock>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Id}" TextAlignment="Center"
                                                               FontSize="17" FontWeight="Bold"
                                                               FontFamily="Technical Italic, Comic Sans MS, Arial"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn Width="250">
                                                <TextBlock Width="250" TextAlignment="Center" Foreground="Orange" FontSize="15" FontWeight="Bold">Date</TextBlock>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Date}" TextAlignment="Center"
                                                               FontSize="17" FontWeight="Bold"
                                                               FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                               Width="250"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn Width="600">
                                                <TextBlock Width="600" TextAlignment="Center" Foreground="Orange" FontSize="15" FontWeight="Bold">Conclusion</TextBlock>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Conclusion}" TextAlignment="Center"
                                                               FontSize="17" FontWeight="Bold"
                                                               FontFamily="Technical Italic, Comic Sans MS, Arial"
                                                               Width="600"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                        </GridView>
                                    </ListView.View>
                                </ListView>
                            </Border>
                        </Grid>

                        <TextBox x:Name="TextBoxData1" Width="260" Height="40" 
                             VerticalAlignment="Center"
                             HorizontalAlignment="Center"
                             Grid.Column="0" Grid.Row="1"
                             FontSize="20"
                             FontFamily="Technical Italic, Comic Sans MS, Arial"
                             Margin="50 20 40 20"
                             TextAlignment="Center"/>

                        <Button x:Name="btnNew" Click="BtnNew_Click" Grid.Column="2" Grid.Row="1" Margin="20 0 20 0">
                            <Button.Background>
                                <LinearGradientBrush>
                                    <GradientStop Color="#FF646464" Offset="0"/>
                                    <GradientStop Color="#FFC1B6B6" Offset="0.5"/>
                                    <GradientStop Color="#FF646464" Offset="1"/>
                                </LinearGradientBrush>
                            </Button.Background>
                            <Button.BorderBrush>
                                <LinearGradientBrush>
                                    <GradientStop Color="#FF646464" Offset="0"/>
                                    <GradientStop Color="#FFC1B6B6" Offset="0.5"/>
                                    <GradientStop Color="#FF646464" Offset="1"/>
                                </LinearGradientBrush>
                            </Button.BorderBrush>
                            <Button.Effect>
                                <DropShadowEffect/>
                            </Button.Effect>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="New" Margin="0 0 30 0"/>
                                <Label Content="{materialDesign:PackIcon download}"
                                    Grid.Column="1"
                                    Foreground="Black"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left"
                                    Margin="0 0 0 0"/>
                            </StackPanel>
                        </Button>

                        <Button x:Name="btnAdd" Click="BtnAdd_Click" Grid.Column="3" Grid.Row="1" Margin="20 0 20 0">
                            <Button.Background>
                                <LinearGradientBrush>
                                    <GradientStop Color="#FF646464" Offset="0"/>
                                    <GradientStop Color="#FFC1B6B6" Offset="0.5"/>
                                    <GradientStop Color="#FF646464" Offset="1"/>
                                </LinearGradientBrush>
                            </Button.Background>
                            <Button.BorderBrush>
                                <LinearGradientBrush>
                                    <GradientStop Color="#FF646464" Offset="0"/>
                                    <GradientStop Color="#FFC1B6B6" Offset="0.5"/>
                                    <GradientStop Color="#FF646464" Offset="1"/>
                                </LinearGradientBrush>
                            </Button.BorderBrush>
                            <Button.Effect>
                                <DropShadowEffect/>
                            </Button.Effect>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Add" Margin="0 0 30 0"/>
                                <Label Content="{materialDesign:PackIcon download}"
                                    Grid.Column="1"
                                    Foreground="Black"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left"
                                    Margin="0 0 0 0"/>
                            </StackPanel>
                        </Button>
                    </Grid>
                </TabItem>
                <TabItem Header="Respiratory">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="0.5*"/>
                            <ColumnDefinition Width="0.5*"/>
                            <ColumnDefinition Width="0.5*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="7*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Grid Grid.Column="0" Grid.ColumnSpan="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="0*"/>
                                <RowDefinition Height="0*"/>
